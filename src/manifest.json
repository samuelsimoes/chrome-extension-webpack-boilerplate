{
  "name": "Chrome Extension Webpack",
  "icons": {
    "128": "/assets/icon-128.png"
  },
  "options_page": "options.html",
  "background": {
    "page": "background.html"
  },
  "browser_action": {
    "default_popup": "popup.html",
    "default_icon": {
      "128": "/assets/icon-128.png"
    }
  },
  "manifest_version": 2,
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'"
}
